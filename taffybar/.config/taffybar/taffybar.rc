style "default" {
    bg[NORMAL]   = "#002b36"
    fg[NORMAL]   = "#586e75"
    text[NORMAL] = "#ffffff"
}

style "notification-button" = "default" {
    text[NORMAL] = "#FF0000"
    fg[NORMAL]   = "#FF0000"
}

widget "Taffybar*" style "default"
widget "*NotificationCloseButton" style "notification-button"
gtk-font-name="Roboto Regular 11"
